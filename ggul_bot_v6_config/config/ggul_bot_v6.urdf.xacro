<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ggul_bot_v6">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import ggul_bot_v6 urdf file -->
    <xacro:include filename="$(find ggul_bot_v6)/urdf/ggul_bot_v6.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="ggul_bot_v6.ros2_control.xacro" />


    <xacro:ggul_bot_v6_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
